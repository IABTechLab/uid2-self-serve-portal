  "scripts": {
    "dev": "run-p start api",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --testPathPattern=src/web/",
    "api": "run-p check-env-file api-watch",
    "check-env-file": "npx ts-node src/checkEnvFile.ts",
    "api-watch": "nodemon -e \".ts\" --exec npm run babel-node-api",
    "babel-node-api": "babel-node -r dotenv/config -x \".ts\" ./src/api/api.ts",
    "build-api": "npx tsc --build ./tsconfig-api.json",
    "test-api": "npx jest --testTimeout=5000 --forceExit --runInBand --config=./src/api/jest.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "stylelint-check": "stylelint-config-prettier-check",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "stylelint": "stylelint \"**/*.scss\"",
    "knex:migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "knex:migrate:down": "knex --knexfile ./knexfile.ts migrate:down",
    "knex:migrate:make": "knex --knexfile ./knexfile.ts migrate:make",
    "knex:seed:make": "knex --knexfile ./knexfile.ts seed:make",
    "knex:seed:run": "knex --knexfile ./knexfile.ts seed:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.s?css": "<rootDir>/src/testHelpers/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|typescript-cookie))"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node-esm"
    }
  },
  "browserslist": {
