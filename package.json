  "scripts": {
    "dev": "run-p start api",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --testPathPattern=src/web/ --runInBand",
    "api": "run-p check-env-file api-watch",
    "check-env-file": "tsx src/checkEnvFile.ts",
    "api-watch": "nodemon -e ts --exec tsx src/api/api.ts",
    "build-api": "node esbuild.config.js",
    "test-api": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --testTimeout=5000 --forceExit --runInBand --config=./src/api/jest.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "stylelint-check": "stylelint-config-prettier-check",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "stylelint": "stylelint \"**/*.scss\"",
    "knex:migrate:latest": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex migrate:latest --knexfile ./knexfile.ts",
    "knex:migrate:rollback": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex migrate:rollback --all --knexfile ./knexfile.ts",
    "knex:migrate:down": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex migrate:down --knexfile ./knexfile.ts",
    "knex:migrate:make": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex migrate:make --knexfile ./knexfile.ts",
    "knex:seed:make": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex seed:make --knexfile ./knexfile.ts",
    "knex:seed:run": "cross-env NODE_OPTIONS=--loader=ts-node/esm knex seed:run --knexfile ./knexfile.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.s?css": "<rootDir>/src/testHelpers/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|typescript-cookie|keycloak-js))"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node-esm"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
