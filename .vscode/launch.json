  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to api",
      "type": "node",
      "request": "attach",
      "port": 6540
    },
    {
      "name": "Launch api via npm",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "api"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\api\\api.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
